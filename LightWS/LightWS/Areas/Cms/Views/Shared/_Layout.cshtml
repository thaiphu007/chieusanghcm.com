<!DOCTYPE html>
<html lang="en">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <!-- Meta, title, CSS, favicons, etc. -->
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>@ViewBag.Title</title>
    <link href="~/Areas/CMS/Content/css/TeamCode.css" rel="stylesheet" />

    <!-- Bootstrap -->
    <link href="~/Areas/CMS/Content/vendors/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Areas/CMS/Content/vendors/bootstrap/dist/css/bootstrap-theme.min.css" rel="stylesheet" />
    <!-- datatables -->
    <link href="~/Areas/CMS/Content/vendors/datatables.net-bs/css/dataTables.bootstrap.min.css" rel="stylesheet" />

    <!-- iCheck -->
    <link href="~/Areas/CMS/Content/vendors/iCheck/skins/flat/green.css" rel="stylesheet" />
    <!-- bootstrap-progressbar -->
    <link href="~/Areas/CMS/Content/vendors/bootstrap-progressbar/css/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet">
    <!-- JQVMap -->
    <link href="~/Areas/CMS/Content/vendors/jqvmap/dist/jqvmap.min.css" rel="stylesheet" />
    <!-- pnotify -->
    <link href="~/Areas/CMS/Content/vendors/pnotify/dist/pnotify.css" rel="stylesheet" />
    <link href="~/Areas/CMS/Content/vendors/pnotify/dist/pnotify.buttons.css" rel="stylesheet" />
    <link href="~/Areas/CMS/Content/vendors/pnotify/dist/pnotify.nonblock.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Areas/CMS/Content/vendors/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- NProgress -->
    <link href="~/Areas/CMS/Content/vendors/nprogress/nprogress.css" rel="stylesheet" />
    <!-- jQuery custom content scroller -->
    <link href="~/Areas/CMS/Content/vendors/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.min.css" rel="stylesheet" />
    <!-- bootstrap-daterangepicker -->    
    <link href="~/Areas/CMS/Content/css/bootstrap-datepicker3.min.css" rel="stylesheet" />
    <link href="~/Areas/CMS/Content/vendors/bootstrap-daterangepicker/daterangepicker.css" rel="stylesheet">
    <!-- Custom Theme Style -->
    <link href="~/Areas/CMS/Content/build/css/custom.min.css" rel="stylesheet" />
    
    
</head>

<body class="nav-md">
    <div class="container body">
        <div class="main_container">
            <div class="col-md-3 left_col">
                @Html.Partial("_LeftCol")
            </div>

            <!-- top navigation -->
            <div class="top_nav">
                @Html.Partial("_NavMenu")
            </div>
            <!-- /top navigation -->
            <!-- page content -->
            <div class="right_col" role="main">
                <div>
                    @RenderBody()
                </div>
            </div>
            <!-- /page content -->
            <!-- footer content -->
            @Html.Partial("_Footer")
            <!-- /footer content -->
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Areas/CMS/Content/ckeditor/ckeditor.js"></script>
    <!-- FastClick -->
    <script src="~/Areas/CMS/Content/vendors/fastclick/lib/fastclick.js"></script>
    <!-- NProgress -->
    <script src="~/Areas/CMS/Content/vendors/nprogress/nprogress.js"></script>
    <!-- datatables -->
    <script src="~/Areas/CMS/Content/vendors/datatables.net/js/jquery.dataTables.min.js"></script>
    <script src="~/Areas/CMS/Content/vendors/datatables.net-bs/js/dataTables.bootstrap.min.js"></script>
    <!-- bootstrap-progressbar -->
    <script src="~/Areas/CMS/Content/vendors/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
    <!-- Flot -->
    <script src="~/Areas/CMS/Content/vendors/Flot/jquery.flot.js"></script>
    <script src="~/Areas/CMS/Content/vendors/Flot/jquery.flot.pie.js"></script>
    <script src="~/Areas/CMS/Content/vendors/Flot/jquery.flot.time.js"></script>
    <script src="~/Areas/CMS/Content/vendors/Flot/jquery.flot.stack.js"></script>
    <script src="~/Areas/CMS/Content/vendors/Flot/jquery.flot.resize.js"></script>
    <!-- Flot plugins -->
    <script src="~/Areas/CMS/Content/vendors/flot.orderbars/js/jquery.flot.orderBars.js"></script>
    <script src="~/Areas/CMS/Content/vendors/flot-spline/js/jquery.flot.spline.min.js"></script>
    <script src="~/Areas/CMS/Content/vendors/flot.curvedlines/curvedLines.js"></script>
    <!-- iCheck -->
    <script src="~/Areas/CMS/Content/vendors/iCheck/icheck.min.js"></script>
    <!-- pnotify -->
    <script src="~/Areas/CMS/Content/vendors/pnotify/dist/pnotify.js"></script>
    <script src="~/Areas/CMS/Content/vendors/pnotify/dist/pnotify.buttons.js"></script>
    <script src="~/Areas/CMS/Content/vendors/pnotify/dist/pnotify.nonblock.js"></script>
    <!-- jQuery custom content scroller -->
    <script src="~/Areas/CMS/Content/vendors/malihu-custom-scrollbar-plugin/jquery.mCustomScrollbar.js"></script>
    <!-- bootstrap-daterangepicker -->
    <script src="~/Areas/CMS/Content/js/bootstrap-datepicker.min.js"></script>
    <script src="~/Areas/CMS/Content/js/bootstrap-datepicker.en-GB.min.js"></script>
    <script src="~/Areas/CMS/Content/vendors/moment/min/moment.min.js"></script>
    <script src="~/Areas/CMS/Content/vendors/bootstrap-daterangepicker/daterangepicker.js"></script>
    <!-- Custom Theme Scripts -->
    <script src="~/Areas/CMS/Content/build/js/custom.js"></script>
    <script src="~/Areas/CMS/Content/js/TeamCode.js"></script>
    @RenderSection("scripts", required: false)

    <div class="divLoading"></div>
</body>
</html>